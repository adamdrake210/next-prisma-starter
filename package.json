{
  "name": "next-prisma-starter",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=12.*"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "yarn db:migrate",
    "start": "next start -p $PORT",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy --preview-feature && yarn db:generate",
    "db:gen:migration": "prisma migrate dev --preview-feature",
    "db:reset": "prisma migrate reset --preview-feature",
    "lint": "prettier --check src"
  },
  "dependencies": {
    "@prisma/client": "2.13.1",
    "bcrypt": "^5.0.0",
    "cookie": "^0.4.1",
    "jsonwebtoken": "^8.5.1",
    "next": "10.0.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-error-boundary": "^3.1.0",
    "react-hook-form": "^6.13.1",
    "redaxios": "^0.3.0",
    "superjson": "^1.4.1"
  },
  "devDependencies": {
    "@prisma/cli": "2.13.1",
    "@tailwindcss/forms": "^0.2.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.0",
    "autoprefixer": "^10.1.0",
    "postcss": "^8.2.1",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.1.3"
  }
}
