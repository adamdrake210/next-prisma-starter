{
  "name": "next-prisma-starter",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "14.16.*"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "yarn db:migrate",
    "start": "next start -p $PORT",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy --preview-feature && yarn db:generate",
    "db:gen:migration": "prisma migrate dev --preview-feature",
    "db:reset": "prisma migrate reset --preview-feature",
    "lint": "eslint src --ext ts,tsx,js,jsx"
  },
  "dependencies": {
    "@prisma/client": "2.22.1",
    "argon2": "^0.27.2",
    "cookie": "^0.4.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "next": "10.2.0",
    "next-connect": "^0.10.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-hook-form": "^7.5.3",
    "redaxios": "^0.4.1",
    "superjson": "^1.4.1",
    "swr": "^0.5.6",
    "tailwindcss": "^2.1.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.3.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.169",
    "@types/node": "^15.0.3",
    "@types/react": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "autoprefixer": "^10.1.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-finiam": "^1.0.4",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "postcss": "^8.2.15",
    "prettier": "^2.3.0",
    "prisma": "^2.22.1",
    "typescript": "^4.1.3"
  }
}
